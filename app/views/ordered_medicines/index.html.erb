<p id="notice"><%= notice %></p>

<h1>Ordered Medicines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordered_medicines.each do |ordered_medicine| %>
      <tr>
        <td><%= link_to 'Show', ordered_medicine %></td>
        <td><%= link_to 'Edit', edit_ordered_medicine_path(ordered_medicine) %></td>
        <td><%= link_to 'Destroy', ordered_medicine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ordered Medicine', new_ordered_medicine_path %>
